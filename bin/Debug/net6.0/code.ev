import(Math)

func("void"; "convert"; "num(a; {0})"; "num(b; {0})"; {
	array("num"; tab; 10000)
	num(n; 0)
	
	while ("&a > 0" {
		set(&a % &b; tab; &n)
		set(a; round(&a / &b; 0))
		set(n; &n + 1)
	})

	num(i; &n - 1)

	while ("&i >= 0"; {
		print(get(&i; tab))
		set(i; &i - 1)
	})
})

