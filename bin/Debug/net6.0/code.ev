func("num"; fib; "num(n; {0})"; {
	if (&n == 1 || &n == 2; {
		num(r; 1)
	})
	else ({
		num(i; 3)
		num(x; 1)
		num(y; 1)
		num(t; 0)

		while ("&i <= &n"; {
			set(t; &x)
			set(x; &x + &y)
			set(y; &t)
			set(i; &i + 1)
		})

		num(r; &x)
	})
})

num(j; 1)

while ("&j <= 15"; {
	print(exec(&j; fib))
	set(j; &j + 1)
})